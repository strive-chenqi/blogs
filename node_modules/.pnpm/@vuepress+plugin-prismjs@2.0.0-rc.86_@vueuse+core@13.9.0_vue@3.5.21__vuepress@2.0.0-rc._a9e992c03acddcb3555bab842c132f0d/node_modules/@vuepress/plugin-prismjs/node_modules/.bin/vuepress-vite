#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chenqi/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@24.5.2_sass-embedded@1.93.0_sass@1.93.0__vue@3.5.21/node_modules/vuepress/bin/node_modules:/home/chenqi/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@24.5.2_sass-embedded@1.93.0_sass@1.93.0__vue@3.5.21/node_modules/vuepress/node_modules:/home/chenqi/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@24.5.2_sass-embedded@1.93.0_sass@1.93.0__vue@3.5.21/node_modules:/home/chenqi/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chenqi/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@24.5.2_sass-embedded@1.93.0_sass@1.93.0__vue@3.5.21/node_modules/vuepress/bin/node_modules:/home/chenqi/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@24.5.2_sass-embedded@1.93.0_sass@1.93.0__vue@3.5.21/node_modules/vuepress/node_modules:/home/chenqi/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@24.5.2_sass-embedded@1.93.0_sass@1.93.0__vue@3.5.21/node_modules:/home/chenqi/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@24.5.2_sass-embedded@1.93.0_sass@1.93.0__vue@3.5.21/node_modules/vuepress/bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@24.5.2_sass-embedded@1.93.0_sass@1.93.0__vue@3.5.21/node_modules/vuepress/bin/vuepress-vite.js" "$@"
fi
